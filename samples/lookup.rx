
Animal = Object spawn
Animal name = "Animal"

// Default inheritance using slot "delegate"
Dog = Animal spawn
fido = Dog spawn
fido name println

// Custom inheritance using slot "parent"
Entity = Object spawn
Entity lookup = method(messageName,
    self parent valueOfSlot(messageName)
)

Dog = Entity spawn
Dog parent = Animal

fido = Entity spawn
fido parent = Dog

fido name println
